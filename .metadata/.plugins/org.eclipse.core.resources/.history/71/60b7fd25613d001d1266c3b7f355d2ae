package OOps;

public class Student extends School {
	int id;
	String name[];
	int learn_hour();
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String[] getName() {
		return name;
	}
	public void setName(String[] name) {
		this.name = name;
	}
	int learn_hour()
	{
		return 6;
	}

}
