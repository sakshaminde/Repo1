package OOps;

public abstract class Vehicles {
	abstract void run();
	abstract int mileage();
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Truck t1= new Truck();
		t1.run();
		Bus b1 = new Bus();
		b1.run();
		Car c1 = new Car();
		c1.run();
		Container c11 = new Container();
		c11.run();
	}

}
