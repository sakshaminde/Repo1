package Methods;

public class Exception1 {
	public static void main(String[] args)
	{
		int num;
		try
		{
			 num = 100/0;
		}
		catch(ArithmeticException e)
		{
			e.printStackTrace();
		}
		finally
		{
			System.out.println("Continue Rest code");
		}
		
	}

}

