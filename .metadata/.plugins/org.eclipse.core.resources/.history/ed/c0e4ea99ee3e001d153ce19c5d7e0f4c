package Methods;

public class Add {
	int add(int a, int b)
	{
		System.out.println("Addition of 2 var is");
		return a+b;
	}
	int add(int a, int c, int d)
	{
		System.out.println("Addition of 3 var is");
		return a+c+d;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Add a1 = new Add();
		a1.add(2, 3);
		a1.add(5, 6, 3);
		
	}

}
