package Methods;

public class Add {
	int add(int a, int b)
	{
		
		return a+b;
	}
	int add(int a, int c, int d)
	{
		
		return a+c+d;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Add a1 = new Add();
		System.out.println((a1.add(5, 6));
		System.out.println(a1.add(3, 6, 7));
		/*a1.add(2, 3);
		a1.add(5, 6, 3);*/
		
	}

}
